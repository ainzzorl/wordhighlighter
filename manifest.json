{
  "manifest_version": 2,
  "name": "Word Highlighter",
  "version": "0.0.1",
  "permissions": ["storage"],
  "applications": {
    "gecko": {
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "scripts": [ "dao.js", "background.js" ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dao.js", "content-script.js"],
      "css": ["wordhighlighter.css"]
    }
  ]
}
